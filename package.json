{
    "name": "fusive-app",
    "productName": "Fusive App",
    "version": "1.0.0",
    "description": "Client's App For Playing Media On Stream",
    "main": "index.js",
    "scripts": {
        "dev": "electron src/index.js",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . fusive-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Fusive App\"",
        "package-linux": "electron-packager . fusive-app --overwrite --asar=true --platform=linux --arch=x64 --icon=./assets/icons/png/1024x1024.png --prune=true --out=release-builds"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fusive/client.git"
    },
    "author": "Jayex Designs",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Fusive/client/issues"
    },
    "homepage": "https://github.com/Fusive/client#readme",
    "devDependencies": {
        "electron": "^13.1.9",
        "electron-packager": "^15.3.0",
        "electron-reload": "^2.0.0-alpha.1"
    }
}
